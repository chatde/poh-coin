# Copy this file to .env and fill in your values
# NEVER commit .env to git — it's in .gitignore

# Your wallet's private key (from MetaMask: Account Details → Export Private Key)
# IMPORTANT: Never share this with anyone
DEPLOYER_PRIVATE_KEY=0x_your_private_key_here

# Basescan API key for contract verification (free at https://basescan.org/myapikey)
BASESCAN_API_KEY=your_basescan_api_key_here
